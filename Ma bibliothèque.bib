  shorttitle = {Interfacial {{Properties}} of {{Polymer Nanocomposites}}},
  author = {Cheng, Shiwang and Carroll, Bobby and Lu, Wei and Fan, Fei and Carrillo, Jan-Michael Y. and Martin, Halie and Holt, Adam P. and Kang, Nam-Goo and Bocharova, Vera and Mays, Jimmy W. and Sumpter, Bobby G. and Dadmun, Mark and Sokolov, Alexei P.},
