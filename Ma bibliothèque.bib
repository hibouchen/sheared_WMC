@article{adjariSlippageEntangledPolymer1994,
  title = {Slippage of an Entangled Polymer Melt on a Grafted Surface},
  author = {Adjari, Armand and {Brochard-Wyart}, Fran{\c c}oise and {de Gennes}, Pierre-Gilles and Leibler, Ludwik and Viovy, Jean-Louis and Rubinstein, Michael},
  year = {1994},
