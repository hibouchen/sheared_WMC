@article{cousinEROSIIBoosted2011,
  title = {{{EROS II}}: {{A}} Boosted Time-of-Flight Reflectometer for Multi-Purposes Applications at the {{Laboratoire L\'eon Brillouin}}},
  shorttitle = {{{EROS II}}},
  author = {Cousin, F. and Ott, F. and Gibert, F. and Menelle, A.},
  year = {2011},
  month = nov,
  journal = {The European Physical Journal Plus},
  volume = {126},
  number = {11},
  pages = {109},
  issn = {2190-5444},
  doi = {10.1140/epjp/i2011-11109-6},
  abstract = {We present the characteristics and performances of the time-of-flight EROS reflectometer, located at Laboratoire L\'eon Brillouin (Saclay, France), which operates on the high-flux Orph\'ee reactor. It is a versatile spectrometer with a horizontal geometry enabling air/liquid, air/solid and solid/liquid measurements. It is mostly dedicated to soft matter projects but welcomes also studies on thin solid films. It is equipped with a multi-disc chopper providing a tunable {$\delta\lambda$}/{$\lambda$} resolution, a collimation system with two deviating supermirrors and slits with flexible apertures allowing any given {$\delta\theta$}/\texttheta{} higher than 0.007, a large sample area, and a detector (either single tube or positioning sensitive detector). The accessible Q min is 0.0026 \AA -1 and the accessible Q max is 0.36 \AA -1 in air/solid or solid/liquid geometries and 0.21 \AA -1 in air/liquid geometry. The spectrometer has been upgraded during the last decade in order to reach a gain in flux of {$\sim$} 50 from its original version. In air/solid geometry, we reach R min = 2\texttimes 10-5 in 1 hour, R min = 8\texttimes 10-7 in 4 hours and R min = 4\texttimes 10-7 in 12 hours on a sample of {$\sim$} 20 cm2 with EROS II, while it was difficult to go below 10-5 with the initial version dating from 1992. In air/liquid or solid/liquid geometries, we show that the main limitation to reach very low reflectivities is the incoherent scattering of the liquid solvent (D2O) . For such geometries, we obtain R min of 5\texttimes 10-6 in 4 hours after incoherent background subtraction. The rejuvenated EROS allows thus measurements of 15-20 samples of a few tens cm2 in a span of 4 days, which corresponds to the minimum time allocated to external users for a given experiment.},
  langid = {english},
  keywords = {Cold Source,Deuterated Water,Incoherent Scattering,Neutron Beam,Silicon Wafer}
}

@article{cousinNeutronReflectivitySoft2018,
  title = {Neutron Reflectivity for Soft Matter},
  author = {Cousin, Fabrice and Chennevi{\`e}re, Alexis},
  year = {2018},
  journal = {EPJ Web of Conferences},
  volume = {188},
  pages = {04001},
  issn = {2100-014X},
  doi = {10.1051/epjconf/201818804001},
  abstract = {Specular neutron reflectivity is a technique enabling the measurement of coherent neutron scattering length density profile perpendicular to the plane of a surface or interface, and thereby the profile of chemical composition. The characteristic sizes that are probed range from around 5\AA{} up 5000 \AA. It is a scattering technique that averages information over the entire surface and it is therefore not possible to obtain information on correlations in the plane of the interface. The specific properties of neutrons (possibility of tuning the contrast by isotopic substitution, negligible absorption, low energy of the incident neutrons) makes it particularly interesting in the fields of soft matter and biophysics. This course is composed of three parts describing respectively its principle, the experimental aspects (diffractometers, samples), and some scientific examples of neutron reflectometry focusing on the use of contrast variation to probe polymeric systems.},
  copyright = {\textcopyright{} The Authors, published by EDP Sciences, 2018},
  langid = {english},
  file = {/home/achennev/Zotero/storage/8F9SK6P4/Cousin et Chennevière - 2018 - Neutron reflectivity for soft matter.pdf;/home/achennev/Zotero/storage/7BC6U7NK/epjconf_jdn2018_04001.html}
}

