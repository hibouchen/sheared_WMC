  title = {Electrolytic Conductivity of Four Imidazolium-Based Room-Temperature Ionic Liquids and the Effect of a Water Impurity},
  author = {Widegren, Jason A. and Saurer, Eric M. and Marsh, Kenneth N. and Magee, Joseph W.},
