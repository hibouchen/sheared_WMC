  author = {Houvenagel, Sophie and Moine, Laurence and Picheth, Guilherme and Dejean, Camille and Br{\^u}let, Annie and Chennevi{\`e}re, Alexis and Faugeras, Vincent and Huang, Nicolas and Couture, Olivier and Tsapis, Nicolas},
