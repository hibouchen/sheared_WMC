@article{madkourUnveilingDynamicsSelfAssembled2017,
  title = {Unveiling the {{Dynamics}} of {{Self-Assembled Layers}} of {{Thin Films}} of {{Poly}}(Vinyl Methyl Ether) ({{PVME}}) by {{Nanosized Relaxation Spectroscopy}}},
  author = {Madkour, Sherif and Szymoniak, Paulina and Heidari, Mojdeh and {von Klitzing}, Regine and Sch{\"o}nhals, Andreas},
