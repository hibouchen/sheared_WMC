@article{huInterfacialMorphologyLowvoltage2013,
  title = {Interfacial Morphology of Low-Voltage Anodic Aluminium Oxide},
  author = {Hu, N. and Dong, X. and He, X. and Argekar, S. and Zhang, Y. and Browning, J. F. and Schaefer, D. W.},
