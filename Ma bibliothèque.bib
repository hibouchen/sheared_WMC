@article{kumarIonTransportSoftening2014,
  title = {Ion Transport and Softening in a Polymerized Ionic Liquid},
  author = {Kumar, Rajeev and Bocharova, Vera and Strelcov, Evgheni and Tselev, Alexander and Kravchenko, Ivan I. and Berdzinski, Stefan and Strehmel, Veronika and Ovchinnikova, Olga S. and Minutolo, Joseph A. and Sangoro, Joshua R. and Agapov, Alexander L. and Sokolov, Alexei P. and Kalinin, Sergei V. and Sumpter, Bobby G.},
  year = {2014},
  month = dec,
  journal = {Nanoscale},
  volume = {7},
  number = {3},
  pages = {947--955},
  publisher = {{The Royal Society of Chemistry}},
  issn = {2040-3372},
  doi = {10.1039/C4NR05491A},
  abstract = {Polymerized ionic liquids (PolyILs) are promising materials for various solid state electronic applications such as dye-sensitized solar cells, lithium batteries, actuators, field-effect transistors, light emitting electrochemical cells, and electrochromic devices. However, fundamental understanding of interconnection between ionic transport and mechanical properties in PolyILs is far from complete. In this work, local charge transport and structural changes in films of a PolyIL are studied using an integrated experiment-theory based approach. Experimental data for the kinetics of charging and steady state current\textendash voltage relations can be explained by taking into account the dissociation of ions under an applied electric field (known as the Wien effect). Onsager's theory of the Wien effect coupled with the Poisson\textendash Nernst\textendash Planck formalism for the charge transport is found to be in excellent agreement with the experimental results. The agreement between the theory and experiments allows us to predict structural properties of the PolyIL films. We have observed significant softening of the PolyIL films beyond certain threshold voltages and formation of holes under a scanning probe microscopy (SPM) tip, through which an electric field was applied. The observed softening is explained by the theory of depression in glass transition temperature resulting from enhanced dissociation of ions with an increase in applied electric field.},
  langid = {english},
  file = {/home/achennev/Zotero/storage/456WBCXJ/Kumar et al. - 2014 - Ion transport and softening in a polymerized ionic.pdf;/home/achennev/Zotero/storage/FK6GC5KU/Kumar et al. - 2014 - Ion transport and softening in a polymerized ionic.pdf}
}

