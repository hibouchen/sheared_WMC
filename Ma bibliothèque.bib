@article{mavronasouPolyVinylPyridine2022,
  title = {Poly(Vinyl Pyridine) and {{Its Quaternized Derivatives}}: {{Understanding Their Solvation}} and {{Solid State Properties}}},
  shorttitle = {Poly(Vinyl Pyridine) and {{Its Quaternized Derivatives}}},
  author = {Mavronasou, Katerina and Zamboulis, Alexandra and Klonos, Panagiotis and Kyritsis, Apostolos and Bikiaris, Dimitrios N. and Papadakis, Raffaello and Deligkiozi, Ioanna},
  year = {2022},
