@article{heresIonTransportGlassy2019,
  title = {Ion {{Transport}} in {{Glassy Polymerized Ionic Liquids}}: {{Unraveling}} the {{Impact}} of the {{Molecular Structure}}},
  shorttitle = {Ion {{Transport}} in {{Glassy Polymerized Ionic Liquids}}},
  author = {Heres, Maximilian and Cosby, Tyler and Mapesa, Emmanuel Urandu and Liu, Hongjun and Berdzinski, Stefan and Strehmel, Veronika and Dadmun, Mark and Paddison, Stephen J. and Sangoro, Joshua},
  year = {2019},
  month = jan,
  journal = {Macromolecules},
  volume = {52},
  number = {1},
  pages = {88--95},
  publisher = {{American Chemical Society}},
  issn = {0024-9297},
  doi = {10.1021/acs.macromol.8b01273},
  abstract = {The impact of molecular structure on ion dynamics and morphology in ammonium- and imidazolium-based glassy polymerized ionic liquids (polyILs) is investigated using broadband dielectric spectroscopy (BDS), wide-angle X-ray scattering (WAXS), and classical molecular dynamics (MD) simulations. It is shown that ammonium-based polyILs exhibit higher dc ionic conductivity at their respective glass transition temperatures (Tg) compared to imidazolium systems. In addition, the length of the alkyl spacer has a more drastic impact on ionic conductivity at comparable time scales of segmental dynamics for ammonium than imidazolium polyILs. Agreement between the characteristic ion diffusion lengths estimated from the dielectric data and the ion-to-ion correlation lengths from the WAXS and all-atom MD simulations is observed. A recently proposed approach is employed to determine ionic mobility in a broad frequency range spanning 5 orders of magnitude below the Tg of polyILs studied, providing access to a regime of diffusivities that is inaccessible to many current experimental techniques. The ion mobility is found to be more sensitive to variation of the molecular structure than to the effective number density of the mobile ions. These results showcase the subtle interplay between molecular structure, morphology, and ion dynamics in polymerized ionic liquids.},
  file = {/home/achennev/Zotero/storage/UJXR3FUQ/Heres et al. - 2019 - Ion Transport in Glassy Polymerized Ionic Liquids.pdf;/home/achennev/Zotero/storage/MUYLA8DV/acs.macromol.html}
}

@article{heresProbingNanoscaleIon2016,
  title = {Probing {{Nanoscale Ion Dynamics}} in {{Ultrathin Films}} of {{Polymerized Ionic Liquids}} by {{Broadband Dielectric Spectroscopy}}},
