@article{chungDirectObservationsSegmental2021,
  title = {Direct {{Observations}} of {{Segmental Dynamics}} at the {{Polymer}}\textendash{{Substrate Interface Enabled}} by {{Localizing Fluorescent Probes}} with {{Polymer Brushes}}},
