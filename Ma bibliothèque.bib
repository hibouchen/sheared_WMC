@article{shaplovRecentAdvancesInnovative2015,
  title = {Recent {{Advances}} in {{Innovative Polymer Electrolytes}} Based on {{Poly}}(Ionic Liquid)s},
  author = {Shaplov, Alexander S. and Marcilla, Rebeca and Mecerreyes, David},
  year = {2015},
  month = sep,
  journal = {Electrochimica Acta},
