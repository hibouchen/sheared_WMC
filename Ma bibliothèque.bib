@article{henotTemperatureControlledSlipPolymer2018,
  title = {Temperature-{{Controlled Slip}} of {{Polymer Melts}} on {{Ideal Substrates}}},
  author = {H{\'e}not, Marceau and Grzelka, Marion and Zhang, Jian and Mariot, Sandrine and Antoniuk, Iurii and Drockenmuller, Eric and L{\'e}ger, Liliane and Restagno, Fr{\'e}d{\'e}ric},
