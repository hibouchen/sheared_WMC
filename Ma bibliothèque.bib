@article{fajardoElectrotunableFrictionIonic2015,
  title = {Electrotunable {{Friction}} with {{Ionic Liquid Lubricants}}: {{How Important Is}} the {{Molecular Structure}} of the {{Ions}}?},
  shorttitle = {Electrotunable {{Friction}} with {{Ionic Liquid Lubricants}}},
  author = {Fajardo, O. Y. and Bresme, Fernando and Kornyshev, Alexei A. and Urbakh, Michael},
  year = {2015},
